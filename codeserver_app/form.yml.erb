<%
app_name = "VSCode Server"
template_root = "/var/www/ood/apps/templates/"
%>
---
title: <%= app_name %>
attributes:
  cluster:
    widget: select
    options:
      <%- CustomQueues.clusters.each do |g| %>
      - "<%= g %>"
      <%- end %>
<% IO.foreach(template_root+"friscos_v2") do |line| %>
<%= line %>
<% end %>    
<% IO.foreach(template_root+"cluster-help") do |line| %>
<%= line %>
<% end %>    
  custom_accpart:
    widget: select
    options:
      <%- CustomAccPart.accpart.each do |g| %>
      - "<%= g %>"
      <%- end %>
<% IO.foreach(template_root+"accpart-help") do |line| %>
<%= line %>
<% end %>    

<% IO.foreach(template_root+"job_params_v3") do |line| %>
<%= line %>
<% end %>    

form:
  - cluster
  - custom_accpart
  - num_cores
  - bc_num_hours
  - memtask
  - gpu_type
  - gpu_count
  - bc_vnc_resolution
  - bc_email_on_started
